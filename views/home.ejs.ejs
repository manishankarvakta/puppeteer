                        <h5>Generate Slip List</h5>
                        <hr>
                        <form action="/generateSlip" method="POST">
                            <div class="mb-3">
                                <label for="">Enter Current Slip Id</label>
                                <input class="form-control" name="currentSlip" type="text">
                            </div>
                            <div class="mb-3">
                                <label for="">Select order</label>
                                <input class="form-check-input" name="order"  type="radio" Value="ASC"> 
                                <label class="form-check-label" for="ascOrder">
                                    ASC
                                </label>
                                <input class="form-check-input" name="order" type="radio" Value="DSC"> 
                                <label class="form-check-label" for="dscOrder">
                                    DSC
                                </label>
                            </div>
                            <div class="mb-3">
                                <label for="">Num of Slip</label>
                                <input class="form-control" name="num" type="number">
                            </div>
                            <div class="mb-3">
                                <button type="submit" class="btn btn-primary">Submit</button>
                            </div>
                          </form>
                    </div>
                   
